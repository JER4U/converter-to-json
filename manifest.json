{
    "manifest-version": 2,
    "name": "My Extension",
    "version": 1.0,
    "description": "Converts table data to JSON",
    "icons": {
        "48": "icon.png"
    },
    "browser_action": {
    "default_icon": {
        "48": "icon.png"
    },
    "default_title": "Converts to JSON",
    "default_popup": "popup.html"
    },
    "permissions": [
        "activeTab",
        "storage",
        "downloads",
        "tabs"
    ]
}